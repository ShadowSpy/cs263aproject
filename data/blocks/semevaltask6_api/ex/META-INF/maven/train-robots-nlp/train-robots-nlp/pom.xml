<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>train-robots-nlp</groupId>
	<artifactId>train-robots-nlp</artifactId>
	<packaging>jar</packaging>
	<version>1.0.0</version>
	<name>Train Robots NLP</name>
	<url>http://maven.apache.org</url>
	<parent>
		<groupId>train-robots</groupId>
		<artifactId>train-robots</artifactId>
		<version>1.0.0</version>
	</parent>
	<dependencies>
		<dependency>
			<groupId>train-robots-core</groupId>
			<artifactId>train-robots-core</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.4</version>
			<scope>test</scope>
		</dependency>
    <dependency>
      <groupId>eu.danieldk.nlp.jitar</groupId>
      <artifactId>jitar</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </dependency>
  </dependencies>
	<build>
		<finalName>train-robots-nlp</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>